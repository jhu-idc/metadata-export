_internal__target_id": "islandora_object"}}, "links": {"related": {"href": "http://digital.library.jhu.edu/jsonapi/node/islandora_object/1705c956-90c7-4f17-a4c1-25b3cd3bafd4/node_type?resourceVersion=id%3A124"}, "self": {"href": "http://digital.library.jhu.edu/jsonapi/node/islandora_object/1705c956-90c7-4f17-a4c1-25b3cd3bafd4/relationships/node_type?resourceVersion=id%3A124"}}}, "revision_uid": {"data": {"type": "user--user", "id": "dfa33910-e475-482d-9fb2-7c268bc2ee7b", "meta": {"drupal_internal__target_id": 534}}, "links": {"related": {"href": "http://digital.library.jhu.edu/jsonapi/node/islandora_object/1705c956-90c7-4f17-a4c1-25b3cd3bafd4/revision_uid?resourceVersion=id%3A124"}, "self": {"href": "http://digital.library.jhu.edu/jsonapi/node/islandora_object/1705c956-90c7-4f17-a4c1-25b3cd3bafd4/relationships/revision_uid?resourceVersion=id%3A124"}}}, "uid": {"data": {"type": "user--user", "id": "dfa33910-e475-482d-9fb2-7c268bc2ee7b", "meta": {"drupal_internal__target_id": 534}}, "links": {"related": {"href": "http://digital.library.jhu.edu/jsonapi/node/islandora_object/1705c956-90c7-4f17-a4c1-25b3cd3bafd4/uid?resourceVersion=id%3A124"}, "self": {"href": "http://digital.library.jhu.edu/jsonapi/node/islandora_object/1705c956-90c7-4f17-a4c1-25b3cd3bafd4/relationships/uid?resourceVersion=id%3A124"}}}, "field_abstract": {"data": [{"type": "taxonomy_term--language", "id": "c5437b6e-77d8-4a23-9dc9-f549b5a4f9aa", "meta": {"value": "Thomas Bourne Turner was born in Frederick, Maryland. He completed his undergraduate work at St. John's College in Annapolis, Maryland, and went on to receive his medical degree from the University of Maryland. Turner joined the faculty at Johns Hopkins Medical School in the late 1920s and became a dean in 1957. He is best known for his work in infectious disease and microbiology. Turner left his position at the medical school during World War II in order to head up the Army's syphilis eradication program. He returned to his professorship afterward. In this history, Turner recounts his education and his career. This oral history is part of the Mame Warren oral histories series.", "drupal_internal__target_id": 776}}], "links": {"related": {"href": "http://digital.library.jhu.edu/jsonapi/node/islandora_object/1705c956-90c7-4f17-a4c1-25b3cd3bafd4/field_abstract?resourceVersion=id%3A124"}, "self": {"href": "http://digital.library.jhu.edu/jsonapi/node/islandora_object/1705c956-90c7-4f17-a4c1-25b3cd3bafd4/relationships/field_abstract?resourceVersion=id%3A124"}}}, "field_access_rights": {"data": [{"type": "taxonomy_term--access_rights", "id": "62cfdefb-9b9a-4039-980b-ef8048fb0c1b", "meta": {"drupal_internal__target_id": 172}}], "links": {"related": {"href": "http://digital.library.jhu.edu/jsonapi/node/islandora_object/1705c956-90c7-4f17-a4c1-25b3cd3bafd4/field_access_rights?resourceVersion=id%3A124"}, "self": {"href": "http://digital.library.jhu.edu/jsonapi/node/islandora_object/1705c956-90c7-4f17-a4c1-25b3cd3bafd4/relationships/field_access_rights?resourceVersion=id%3A124"}}}, "field_access_terms": {"data": [{"type": "taxonomy_term--islandora_access", "id": "ced3af6c-fa7c-43a0-98a3-9a0be2bb2b46", "meta": {"drupal_internal__target_id": 772}}], "links": {"related": {"href": "http://digital.library.jhu.edu/jsonapi/node/islandora_object/1705c956-90c7-4f17-a4c1-25b3cd3bafd4/field_access_terms?resourceVersion=id%3A124"}, "self": {"href": "http://digital.library.jhu.edu/jsonapi/node/islandora_object/1705c956-90c7-4f17-a4c1-25b3cd3bafd4/relationships/field_access_terms?resourceVersion=id%3A124"}}}, "field_alternative_title": {"data": [], "links": {"related": {"href": "http://digital.library.jhu.edu/jsonapi/node/islandora_object/1705c956-90c7-4f17-a4c1-25b3cd3bafd4/field_alternative_title?resourceVersion=id%3A124"}, "self": {"href": "http://digital.library.jhu.edu/jsonapi/node/islandora_object/1705c956-90c7-4f17-a4c1-25b3cd3bafd4/relationships/field_alternative_title?resourceVersion=id%3A124"}}}, "field_contributor": {"