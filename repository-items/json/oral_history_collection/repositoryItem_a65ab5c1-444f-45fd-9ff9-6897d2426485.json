al_internal__target_id": "islandora_object"}}, "links": {"related": {"href": "http://digital.library.jhu.edu/jsonapi/node/islandora_object/a65ab5c1-444f-45fd-9ff9-6897d2426485/node_type?resourceVersion=id%3A111"}, "self": {"href": "http://digital.library.jhu.edu/jsonapi/node/islandora_object/a65ab5c1-444f-45fd-9ff9-6897d2426485/relationships/node_type?resourceVersion=id%3A111"}}}, "revision_uid": {"data": {"type": "user--user", "id": "dfa33910-e475-482d-9fb2-7c268bc2ee7b", "meta": {"drupal_internal__target_id": 534}}, "links": {"related": {"href": "http://digital.library.jhu.edu/jsonapi/node/islandora_object/a65ab5c1-444f-45fd-9ff9-6897d2426485/revision_uid?resourceVersion=id%3A111"}, "self": {"href": "http://digital.library.jhu.edu/jsonapi/node/islandora_object/a65ab5c1-444f-45fd-9ff9-6897d2426485/relationships/revision_uid?resourceVersion=id%3A111"}}}, "uid": {"data": {"type": "user--user", "id": "dfa33910-e475-482d-9fb2-7c268bc2ee7b", "meta": {"drupal_internal__target_id": 534}}, "links": {"related": {"href": "http://digital.library.jhu.edu/jsonapi/node/islandora_object/a65ab5c1-444f-45fd-9ff9-6897d2426485/uid?resourceVersion=id%3A111"}, "self": {"href": "http://digital.library.jhu.edu/jsonapi/node/islandora_object/a65ab5c1-444f-45fd-9ff9-6897d2426485/relationships/uid?resourceVersion=id%3A111"}}}, "field_abstract": {"data": [{"type": "taxonomy_term--language", "id": "c5437b6e-77d8-4a23-9dc9-f549b5a4f9aa", "meta": {"value": "Susan Terranova ('76 BA, '77 MA SAIS) was a member of one of the first undergraduate classes to admit women and was the first woman to join the Johns Hopkins ROTC program. After graduating from the School of Advanced International Studies in 1977, Terranova embarked on a career as an Army officer, teaching at West Point and serving in Korea, Germany, the United States, and Colombia. After retiring from the Army, she began a second career as a foreign languages teacher. In this interview, Terranova recounts her early family life, ROTC at Hopkins during her time, and her career in the Army and as a teacher. This oral history is part of the Hopkins Retrospective oral histories series.", "drupal_internal__target_id": 776}}], "links": {"related": {"href": "http://digital.library.jhu.edu/jsonapi/node/islandora_object/a65ab5c1-444f-45fd-9ff9-6897d2426485/field_abstract?resourceVersion=id%3A111"}, "self": {"href": "http://digital.library.jhu.edu/jsonapi/node/islandora_object/a65ab5c1-444f-45fd-9ff9-6897d2426485/relationships/field_abstract?resourceVersion=id%3A111"}}}, "field_access_rights": {"data": [{"type": "taxonomy_term--access_rights", "id": "62cfdefb-9b9a-4039-980b-ef8048fb0c1b", "meta": {"drupal_internal__target_id": 172}}], "links": {"related": {"href": "http://digital.library.jhu.edu/jsonapi/node/islandora_object/a65ab5c1-444f-45fd-9ff9-6897d2426485/field_access_rights?resourceVersion=id%3A111"}, "self": {"href": "http://digital.library.jhu.edu/jsonapi/node/islandora_object/a65ab5c1-444f-45fd-9ff9-6897d2426485/relationships/field_access_rights?resourceVersion=id%3A111"}}}, "field_access_terms": {"data": [{"type": "taxonomy_term--islandora_access", "id": "ced3af6c-fa7c-43a0-98a3-9a0be2bb2b46", "meta": {"drupal_internal__target_id": 772}}], "links": {"related": {"href": "http://digital.library.jhu.edu/jsonapi/node/islandora_object/a65ab5c1-444f-45fd-9ff9-6897d2426485/field_access_terms?resourceVersion=id%3A111"}, "self": {"href": "http://digital.library.jhu.edu/jsonapi/node/islandora_object/a65ab5c1-444f-45fd-9ff9-6897d2426485/relationships/field_access_terms?resourceVersion=id%3A111"}}}, "field_alternative_title": {"data": [], "links": {"related": {"href": "http://digital.library.jhu.edu/jsonapi/node/islandora_object/a65ab5c1-444f-45fd-9ff9-6897d2426485/field_alternative_title?resourceVersion=id%3A111"}, "self": {"href": "http://digital.library.jhu.edu/jsonapi/node/islandora_object/a65ab5c1-444f-45fd-9ff9-6897d2426485/relationships/field_alternative_title?resourceVersion=id%3A111"}}}, "field_contributo