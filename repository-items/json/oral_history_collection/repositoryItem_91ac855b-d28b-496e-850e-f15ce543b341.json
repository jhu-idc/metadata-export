ta": {"drupal_internal__target_id": "islandora_object"}}, "links": {"related": {"href": "http://digital.library.jhu.edu/jsonapi/node/islandora_object/91ac855b-d28b-496e-850e-f15ce543b341/node_type?resourceVersion=id%3A163"}, "self": {"href": "http://digital.library.jhu.edu/jsonapi/node/islandora_object/91ac855b-d28b-496e-850e-f15ce543b341/relationships/node_type?resourceVersion=id%3A163"}}}, "revision_uid": {"data": {"type": "user--user", "id": "dfa33910-e475-482d-9fb2-7c268bc2ee7b", "meta": {"drupal_internal__target_id": 534}}, "links": {"related": {"href": "http://digital.library.jhu.edu/jsonapi/node/islandora_object/91ac855b-d28b-496e-850e-f15ce543b341/revision_uid?resourceVersion=id%3A163"}, "self": {"href": "http://digital.library.jhu.edu/jsonapi/node/islandora_object/91ac855b-d28b-496e-850e-f15ce543b341/relationships/revision_uid?resourceVersion=id%3A163"}}}, "uid": {"data": {"type": "user--user", "id": "dfa33910-e475-482d-9fb2-7c268bc2ee7b", "meta": {"drupal_internal__target_id": 534}}, "links": {"related": {"href": "http://digital.library.jhu.edu/jsonapi/node/islandora_object/91ac855b-d28b-496e-850e-f15ce543b341/uid?resourceVersion=id%3A163"}, "self": {"href": "http://digital.library.jhu.edu/jsonapi/node/islandora_object/91ac855b-d28b-496e-850e-f15ce543b341/relationships/uid?resourceVersion=id%3A163"}}}, "field_abstract": {"data": [{"type": "taxonomy_term--language", "id": "c5437b6e-77d8-4a23-9dc9-f549b5a4f9aa", "meta": {"value": "Dr. Donald Henderson was born in Lakewood, Ohio on September 7, 1928. He received his bachelor's degree from Oberlin College, his M.D. from the University of Rochester School of Medicine, and his M.P.H (Masters of Public Health) from the Johns Hopkins Bloomberg School of Public Health. Henderson headed the World Health Organization's Global Smallpox Eradication Campaign beginning in 1966, which successfully saw the eradication of smallpox from the entire world by 1977. After his tenure with the WHO, Henderson became dean of the Johns Hopkins School of Public Health and acted as a senior advisor to many federal departments and commissions related to health and safety. He has received numerous honorary degrees, awards, and honors, including the Presidential Medal of Freedom (2002). In this oral history, Henderson describes his career as an epidemiologist, his relationship to Johns Hopkins, and the history of the School of Public Health. This oral history is part of the Mame Warren oral histories series.", "drupal_internal__target_id": 776}}], "links": {"related": {"href": "http://digital.library.jhu.edu/jsonapi/node/islandora_object/91ac855b-d28b-496e-850e-f15ce543b341/field_abstract?resourceVersion=id%3A163"}, "self": {"href": "http://digital.library.jhu.edu/jsonapi/node/islandora_object/91ac855b-d28b-496e-850e-f15ce543b341/relationships/field_abstract?resourceVersion=id%3A163"}}}, "field_access_rights": {"data": [{"type": "taxonomy_term--access_rights", "id": "62cfdefb-9b9a-4039-980b-ef8048fb0c1b", "meta": {"drupal_internal__target_id": 172}}], "links": {"related": {"href": "http://digital.library.jhu.edu/jsonapi/node/islandora_object/91ac855b-d28b-496e-850e-f15ce543b341/field_access_rights?resourceVersion=id%3A163"}, "self": {"href": "http://digital.library.jhu.edu/jsonapi/node/islandora_object/91ac855b-d28b-496e-850e-f15ce543b341/relationships/field_access_rights?resourceVersion=id%3A163"}}}, "field_access_terms": {"data": [{"type": "taxonomy_term--islandora_access", "id": "ced3af6c-fa7c-43a0-98a3-9a0be2bb2b46", "meta": {"drupal_internal__target_id": 772}}], "links": {"related": {"href": "http://digital.library.jhu.edu/jsonapi/node/islandora_object/91ac855b-d28b-496e-850e-f15ce543b341/field_access_terms?resourceVersion=id%3A163"}, "self": {"href": "http://digital.library.jhu.edu/jsonapi/node/islandora_object/91ac855b-d28b-496e-850e-f15ce543b341/relationships/field_access_terms?resourceVersion=id%3A163"}}}, "field_alternative_title": {"data": [], "links": {"related": {"href": "http://digital